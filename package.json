{
  "name": "eslint-config-binden-js",
  "version": "2.0.0",
  "description": "Shareable ESLint config",
  "main": ".eslintrc.json",
  "type": "commonjs",
  "types": "index.d.ts",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "commitlint": "commitlint --verbose --from=$( git rev-list --max-parents=0 $( git rev-parse --abbrev-ref HEAD ) )",
    "install:clean": "npm install",
    "preinstall:clean": "rm -rf node_modules package-lock.json",
    "prepare": "husky",
    "prettier": "prettier -c .",
    "prettier:write": "prettier -c -w ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binden-js/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "binden"
  ],
  "author": "Sergey Bakulin <sergey.bakulin.github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binden-js/eslint-config/issues"
  },
  "homepage": "https://github.com/binden-js/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "devDependencies": {
    "@binden/commitlint-config": "^2.0.0",
    "@binden/semantic-release-config": "^3.0.0",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5"
  },
  "funding": {
    "type": "Coinbase Commerce",
    "url": "https://commerce.coinbase.com/checkout/eddfb0f7-20e7-4e0d-8e04-232bbdf0b397"
  }
}
